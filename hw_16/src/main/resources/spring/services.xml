<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="carService" class="org.example.service.impl.CarServiceImpl">
        <constructor-arg name="carDao" ref="carDao"/>
    </bean>

    <bean id="garageService" class="org.example.service.impl.GarageServiceImpl">
    <constructor-arg name="garageDao" ref="garageDao"/>
</bean>
</beans>